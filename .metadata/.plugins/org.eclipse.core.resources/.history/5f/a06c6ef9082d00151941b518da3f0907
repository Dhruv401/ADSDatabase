package hibernate_java_files;

public class DONOR {

	
	public DONOR()
	{
		
		
	}
	
	private int DONOR_ID;
	
	private String DONOR_NAME;
	
	private String DONOR_ADDRESS;
	
	private long DONOR_DOB;
	
	private int DONOR_AGE;
	
	private String DONOR_PHONE;
	
	private String DONOR_DETAILS;
	
	private String USER_ID;

	public int getDONOR_ID() {
		return DONOR_ID;
	}

	public void setDONOR_ID(int dONOR_ID) {
		DONOR_ID = dONOR_ID;
	}

	public String getDONOR_NAME() {
		return DONOR_NAME;
	}

	public void setDONOR_NAME(String dONOR_NAME) {
		DONOR_NAME = dONOR_NAME;
	}

	public String getDONOR_ADDRESS() {
		return DONOR_ADDRESS;
	}

	public void setDONOR_ADDRESS(String dONOR_ADDRESS) {
		DONOR_ADDRESS = dONOR_ADDRESS;
	}

	public long getDONOR_DOB() {
		return DONOR_DOB;
	}

	public void setDONOR_DOB(long dONOR_DOB) {
		DONOR_DOB = dONOR_DOB;
	}

	public int getDONOR_AGE() {
		return DONOR_AGE;
	}

	public void setDONOR_AGE(int dONOR_AGE) {
		DONOR_AGE = dONOR_AGE;
	}

	public String getDONOR_PHONE() {
		return DONOR_PHONE;
	}

	public void setDONOR_PHONE(String dONOR_PHONE) {
		DONOR_PHONE = dONOR_PHONE;
	}

	public String getDONOR_DETAILS() {
		return DONOR_DETAILS;
	}

	public void setDONOR_DETAILS(String dONOR_DETAILS) {
		DONOR_DETAILS = dONOR_DETAILS;
	}

	public String getUSER_ID() {
		return USER_ID;
	}

	public void setUSER_ID(String uSER_ID) {
		USER_ID = uSER_ID;
	}

	public DONOR( String dONOR_NAME, String dONOR_ADDRESS, long dONOR_DOB, int dONOR_AGE,
			String dONOR_PHONE, String dONOR_DETAILS, String uSER_ID) {
		
		
		DONOR_NAME = dONOR_NAME;
		DONOR_ADDRESS = dONOR_ADDRESS;
		DONOR_DOB = dONOR_DOB;
		DONOR_AGE = dONOR_AGE;
		DONOR_PHONE = dONOR_PHONE;
		DONOR_DETAILS = dONOR_DETAILS;
		USER_ID = uSER_ID;
	}
	
		
	
	
}
