<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="hibernate_java_files.FUND" table="FUND">
      <meta attribute="class-description">
         This class contains the fund details. 
      </meta>
      <id name="FUND_ID" column="FUND_ID" type="int">
         <generator class="sequence">
            <param name="sequence">SEQUENCE1</param>
          </generator>
      </id>
    
    <!--   <property name="ORG_ID" column="ORG_ID" type="string"/> --> 
      <property name="FUND_AMOUNT" column="FUND_AMOUNT" type="int"/>
      <property name="TOTAL_FUND_AMOUNT" column="TOTAL_FUND_AMOUNT" type="int"/>
      <property name="ORG_ID" column="ORG_ID" type="int"/>
     
    
      
   </class>
</hibernate-mapping>