<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="hibernate_java_files.ORGANIZATION_EXPENSE" table="ORGANIZATION_EXPENSE">
      <meta attribute="class-description">
         This class contains the organization expense details. 
      </meta>
      <id name="EXPENSE_ID" column="EXPENSE_ID" type="int">
         <generator class="sequence">
            <param name="sequence">SEQUENCE1</param>
          </generator>
      </id>
    
    <!--   <property name="ORG_ID" column="ORG_ID" type="string"/> --> 
      <property name="EXPENSE_TYPE" column="EXPENSE_TYPE" type="string"/>
      <property name="EXPENSE_DETAILS" column="EXPENSE_DETAILS" type="string"/>
      <property name="EXPENSE_NAME" column="EXPENSE_NAME" type="string"/>
      <property name="EXPENSE_DATE" column="EXPENSE_DATE" type="string"/>
      <property name="ORG_ID" column="ORG_ID" type="int"/>
    
      
   </class>
</hibernate-mapping>